            "command": "python",
            "args": [
                "${workspaceFolder}\\.vscode\\asm-runner.py",
                "${config:winmips64.path}",
                "${file}"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
